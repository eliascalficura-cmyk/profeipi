{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "ELEVEN_API_KEY": "@eleven_api_key",
    "ELEVEN_VOICE_ID": "@eleven_voice_id"
  }
}
